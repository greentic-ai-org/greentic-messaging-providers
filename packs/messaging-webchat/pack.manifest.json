{
  "name": "messaging-webchat",
  "version": "0.4.9",
  "description": "Provider-core WebChat messaging pack (send + ingest).",
  "kind": "application",
  "publisher": "Greentic",
  "config_schema": {
    "provider_config": {
      "format": "json",
      "path": "schemas/messaging/webchat/config.schema.json"
    }
  },
  "components": [
    "messaging-provider-webchat"
  ],
  "extensions": {
    "greentic.provider-extension.v1": {
      "kind": "greentic.provider-extension.v1",
      "version": "0.4.9",
      "inline": {
        "providers": [
          {
            "provider_type": "messaging.webchat",
            "capabilities": [
              "messaging"
            ],
            "ops": [
              "send",
              "ingest"
            ],
            "config_schema_ref": "schemas/messaging/webchat/config.schema.json",
            "state_schema_ref": "",
            "runtime": {
              "component_ref": "messaging-provider-webchat",
              "export": "schema-core-api",
              "world": "greentic:provider/schema-core@1.0.0"
            },
            "docs_ref": ""
          }
        ]
      }
    }
  },
  "secret_requirements": []
}
