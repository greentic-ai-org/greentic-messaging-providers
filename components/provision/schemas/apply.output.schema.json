{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "ok": {
      "type": "boolean"
    },
    "dry_run": {
      "type": "boolean"
    },
    "actions": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "action_type": {
            "type": "string"
          },
          "scope": {
            "type": "string"
          },
          "key": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "message": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "action_type",
          "status"
        ]
      }
    },
    "summary": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "config_keys_written": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "secret_keys_written": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": [
    "ok",
    "dry_run"
  ]
}
